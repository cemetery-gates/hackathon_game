{
  "name": "asterroyale",
  "version": "0.1.0",
  "description": "Multiplayer Asteroids",
  "main": "index.js",
  "scripts": {
    "init:client": "npm i --prefix client",
    "init:server": "npm i --prefix server",
    "init:view": "npm i --prefix view",
    "init": "concurrently \"npm run init:client\" \"npm run init:server\" \"npm run init:view\"",
    "start": "docker-compose start",
    "up:server": "docker-compose up server",
    "up:client": "docker-compose up client",
    "up:view": "docker-compose up view",
    "up:nginx": "docker-compose up nginx",
    "up": "docker-compose up -d",
    "stop": "docker-compose stop",
    "down": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git@github.adtran.com:Hackathon/AsterRoyale.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
